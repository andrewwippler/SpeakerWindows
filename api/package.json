{
  "name": "api2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --ignore-ts-errors",
    "start": "node server.js",
    "test": "NODE_ENV=test node ace test",
    "test:coverage": "c8 npm run test",
    "coverage": "c8 check-coverage --lines 95 node ./test.ts && c8 report",
    "lint": "eslint . ",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "@adonisjs/eslint-config/app"
    ],
    "rules": {
      "@typescript-eslint/explicit-member-accessibility": "off",
      "unicorn/filename-case": "off",
      "@typescript-eslint/no-shadow": "off"
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": "@adonisjs/prettier-config",
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.1.2",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^3.0.3",
    "@japa/file-system": "^2.3.2",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.5.0",
    "@swc/core": "^1.11.9",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.4.2",
    "@types/proxy-addr": "^2.0.3",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^11.0.0",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.6.0",
    "@adonisjs/bouncer": "^3.1.6",
    "@adonisjs/core": "^6.20.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/limiter": "^2.4.0",
    "@adonisjs/lucid": "^21.8.2",
    "@adonisjs/redis": "^9.2.0",
    "@adonisjs/validator": "^13.0.2",
    "@vinejs/vine": "^3.0.1",
    "@xenova/transformers": "^2.17.2",
    "argon2": "^0.43.1",
    "bcrypt": "^6.0.0",
    "lodash": "^4.17.23",
    "luxon": "^3.7.2",
    "mysql2": "^3.17.1",
    "pg": "^8.18.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0"
  },
  "type": "module",
  "imports": {
    "#jobs/*": "./app/jobs/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#app/*": "./app/*.js",
    "#database/*": "./database/*.js",
    "#contracts/*": "./contracts/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#policies/*": "./app/policies/*.js"
  }
}
